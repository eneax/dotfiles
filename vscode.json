    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
